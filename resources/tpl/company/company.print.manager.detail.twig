{% if app.request.get('layout') %}
    {% if app.request.get('layout') == 'print-aushg' %}
        {% include ('company/company.print.detail.public.twig') %}
    {% elseif app.request.get('layout') == 'print-chf' %}
    {% elseif app.request.get('layout') == 'print-dtl' %}   
        {% include ('company/company.print.detail.detail.twig') %}
    {% elseif app.request.get('layout') == 'print-list' %}
        {% include ('company/company.print.detail.list.twig') %}
    {% endif %}

{% else %}
    {% include ('company/company.print.detail.public.twig') %}
{% endif %}
