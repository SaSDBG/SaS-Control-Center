{% extends 'print/print.twig' %}

{% block additional_meta %}
    <strong>Datens&auml;tze:</strong> {{ companies | length }}<br>
    {% include('company/company.print.manager.meta.twig') %}
{% endblock %}


{% block content %}
    {% for company in companies %}
        <h2>{{ company.name | e }}</h2>
        {% include ('company/company.print.manager.detail.twig') %}
        <div class="page-break"></div>
    {% endfor %}
{% endblock %}